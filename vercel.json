{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/applicants/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/events/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/autherize/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}